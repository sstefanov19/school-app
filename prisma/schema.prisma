generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Classes {
    id Int @id @default(autoincrement())
    subject String
    teacher String
    grade Int
}



model Student {
    account_id Int @id @default(autoincrement())
    name String
    grade Int
    password String
}

model Teacher {
    account_id Int @id @default(autoincrement())
    name String
}

model Assignement {
    id Int @id @default(autoincrement())
    title String
    description String
    due_date DateTime
    grade Int

}
